<div class="personal-profile">
    <form>
        <label for="first-name">Họ</label>
        <input type='text' id="first-name" name="first-name" placeholder='Họ' value="User">
        <br>
        <label for="last-name">Tên</label>
        <input type='text' id="last-name" name="last-name" placeholder='Tên' value="Name">
        <br>
        <label for="email">Email</label>
        <input type='email' id="email" name="email" placeholder="Email"  value='Username@gmail.com' >
        <br>
        <label for="address">Địa chỉ</label>
        <input type="text" id="address" name="address" placeholder="Địa chỉ" value="134 Cách mạng tháng 8">
        <br>
        <label for="gender">Giới tính</label>
        <input type="text" id="gender" name="gender" placeholder="Giới tính" value="Nam">
        <br>
        <label for="date-of-birth">Ngày sinh</label>
        <input type="text" id="date-of-birth" name="date-of-birth" placeholder="Ngày sinh" value="11/11/2001">
    </form>
</div>